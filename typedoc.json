{
  "name": "ChemPare Documentation",
  "entryPoints": ["./src/**/*.ts", "./src/**/*.tsx"],
  "plugin": [
    "typedoc-plugin-rename-defaults",
    "typedoc-plugin-coverage",
    "typedoc-plugin-include-example",
    "typedoc-plugin-dt-links",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-missing-exports"
  ],
  "entryPointStrategy": "expand",
  "lightHighlightTheme": "light-plus",
  "collapseInternalModule": true,
  "cleanOutputDir": true,
  "pretty": true,
  "searchCategoryBoosts": {
    "Component": 2,
    "Helper": 1.1,
    "Supplier": 1.1
  },
  "searchGroupBoosts": {
    "Classes": 1.5
  },
  "markdownItOptions": {
    "html": true
  },
  "useTsLinkResolution": true,
  "out": "docs",
  "exclude": ["**/node_modules/**", "**/__tests__/**", "**/__mocks__/**"],

  "useFirstParagraphOfCommentAsSummary": true,
  "includeHierarchySummary": true,
  "excludeTags": ["@internal", "@private", "@protected"],
  "includeVersion": true,
  "hideGenerator": true,
  "readme": "README.md",
  "navigationLinks": {
    "GitHub": "https://github.com/jhyland87/chem-crawler"
  },
  "searchInComments": true,
  "sort": ["source-order"],
  "excludeReferences": false,
  "excludeProtected": false,
  "excludePrivate": false,
  "excludeNotDocumented": false,
  "excludeInternal": false,
  "excludeExternals": false,
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": true
  },
  "sourceLinkTemplate": "https://github.com/jhyland87/chem-crawler/blob/main/{path}#L{line}",
  "disableSources": false,
  "categorizeByGroup": true,
  "defaultCategory": "Supplier",
  "categoryOrder": [
    "Supplier",
    "Components",
    "Utils",
    "Hooks",
    "Types",
    "Context",
    "*"
  ],
  "options": "./typedoc-conf.js"
}