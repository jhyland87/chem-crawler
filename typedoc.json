{
  "entryPoints": ["./src/**/*.ts", "./src/**/*.tsx"],
  "plugin": [
    //"@mxssfd/typedoc-theme",
    //"typedoc-github-theme",
    //"typedoc-theme-hierarchy",
    "typedoc-unhoax-theme",
    "typedoc-plugin-rename-defaults",
    "typedoc-plugin-coverage",
    "typedoc-plugin-include-example",
    "typedoc-plugin-dt-links",
    "typedoc-plugin-mdn-links",
  ],
  //"theme": "hierarchy",
  "entryPointStrategy": "expand",
  "lightHighlightTheme": "light-plus",
  "cleanOutputDir": true,
  "groupReferencesByType": true,
  "searchCategoryBoosts": {
    "Component": 2,
    "Helper": 1.1,
    "Supplier": 1.1
  },
  "searchGroupBoosts": {
    "Classes": 1.5
  },
  "markdownItOptions": {
    "html": true
  },
  "useTsLinkResolution": true,
  "out": "docs",
  "exclude": ["**/node_modules/**", "**/__tests__/**"],
  "name": "Chem Crawler Documentation",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "excludeNotDocumented": false,
  "useFirstParagraphOfCommentAsSummary": true,
  "excludeReferences": true,
  "includeHierarchySummary": true,
  "excludeTags": ["@internal", "@private", "@protected"],
  "includeVersion": true,
  "hideGenerator": true,
  "readme": "README.md",
  "navigationLinks": {
    "GitHub": "https://github.com/jhyland87/chem-crawler"
  },
  "searchInComments": true,
  "sort": ["source-order"],
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": false
  },
  "sourceLinkTemplate": "https://github.com/jhyland87/chem-crawler/blob/main/{path}#L{line}",
  "disableSources": false,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Components",
    "Hooks",
    "Utils",
    "Types",
    "Context",
    "*"
  ]
}