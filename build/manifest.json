{
    "manifest_version": 3,
    "name": "Chem Crawler",
    "version": "0.0.1",
    "description": "A simple React app as a Chrome extension",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["scripting", "activeTab",  "*://*.*/*"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "service-worker.js"
    }
  }